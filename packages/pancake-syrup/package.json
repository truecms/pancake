{
	"name": "@truecms/syrup",
	"version": "2.0.0",
	"description": "Syrup helps you select the right pancake modules.",
	"packageManager": "pnpm@8.15.9",
	"keywords": [
		"npm",
		"flat",
		"peerDependencies",
		"gov.au",
		"auds",
		"Commonwealth of Australia",
		"design guide",
		"design system",
		"cli",
		"pancake-plugin"
	],
	"engines": {
		"node": ">=22",
		"npm": ">=10"
	},
	"main": "./dist/syrup.js",
	"bin": {
		"syrup": "./bin/syrup"
	},
	"scripts": {
		"prepublish": "pnpm run build && pnpm run test",
		"build": "pnpm run build:js",
		"build:watch": "pnpm run build:js && pnpm exec jest --coverage",
		"build:js": "pnpm exec babel src --out-dir dist",
		"test": "pnpm -w run test:unit-test",
		"watch": "pnpm run build:watch && pnpm exec onchange 'src/*.js' '__tests__/*.js' -- pnpm run build:watch"
	},
	"jest": {
		"testEnvironment": "node",
		"testRegex": "\\.spec.js$",
		"collectCoverageFrom": [
			"src/*.js"
		],
		"coverageThreshold": {
			"global": {
				"branches": 0,
				"functions": 0,
				"lines": 0,
				"statements": 0
			}
		}
	},
	"files": [
		"bin/*",
		"dist/*",
		"settings.json"
	],
	"dependencies": {
		"@truecms/pancake": "~2.0.0",
		"inquirer": "6.2.2",
		"request": "^2.88.0",
		"strip-ansi": "5.1.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.3.4",
		"@babel/preset-env": "^7.3.4",
		"del": "^4.0.0",
		"dirsum": "^0.1.1",
		"jest-cli": "^24.5.0",
		"onchange": "^5.2.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/govau/pancake.git"
	},
	"homepage": "https://github.com/govau/pancake",
	"bugs": {
		"url": "https://github.com/govau/pancake/issues",
		"email": "designsystem@digital.gov.au"
	},
	"author": {
		"name": "Commonwealth of Australia, DTA",
		"email": "designsystem@digital.gov.au",
		"url": "https://www.dta.gov.au/"
	},
	"contributors": [
		{
			"name": "Dominik Wilkowski",
			"email": "hi@dominik-wilkowski.com",
			"url": "https://dominik-wilkowski.com/"
		}
	],
	"license": "MIT"
}
